extends template

block pageVars
    - var pageTitle    = "Apps"
    - var activeMenu   = "apps"



append pageConfig
    - var contentLayout = 'content-right-sidebar'
    - var sidebarSticky = true
    //-OPTIONS:- true, false | For sticky(floating) sidebar, will affect only if there is a sidebar

append breadcrumbs
    +breadcrumbs([{url:"index.html",name:"Home"}, {name:"Apps"}])



//- Include page content in content block
append content
    include ../contents/documentation-apps.html

append sidebar
    include ../contents/sidebar/documentation-apps-sidebar.html
