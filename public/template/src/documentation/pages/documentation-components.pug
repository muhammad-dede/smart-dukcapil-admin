extends template

block pageVars
    - var pageTitle    = "Components"
    - var activeMenu   = "components"



append pageConfig
    - var contentLayout = 'content-right-sidebar'
    - var sidebarSticky = true
    //-OPTIONS:- true, false | For sticky(floating) sidebar, will affect only if there is a sidebar

append breadcrumbs
    +breadcrumbs([{url:"index.html",name:"Home"}, {name:"Component"}])



//- Include page content in content block
append content
    include ../contents/documentation-components.html

append sidebar
    include ../contents/sidebar/documentation-components-sidebar.html
