extends template

block pageVars
    - var pageTitle    = "Extensions"
    - var activeMenu   = "extensions"



append pageConfig
    - var contentLayout = 'content-right-sidebar'
    - var sidebarSticky = true
    //-OPTIONS:- true, false | For sticky(floating) sidebar, will affect only if there is a sidebar

append breadcrumbs
    +breadcrumbs([{url:"index.html",name:"Home"}, {name:"Extensions"}])



//- Include page content in content block
append content
    include ../contents/documentation-extensions.html

append sidebar
    include ../contents/sidebar/documentation-extensions-sidebar.html
