extends template

block pageVars
    - var pageTitle    = "Form Elements"
    - var activeMenu   = "form-elements"



append pageConfig
    - var contentLayout = 'content-right-sidebar'
    - var sidebarSticky = true
    //-OPTIONS:- true, false | For sticky(floating) sidebar, will affect only if there is a sidebar

append breadcrumbs
    +breadcrumbs([{url:"/",name:"Home"}, {name:"Forms"}, {name:"Form Elements"}])



//- Include page content in content block
append content
    include ../contents/documentation-form-elements.html

append sidebar
    include ../contents/sidebar/documentation-form-elements-sidebar.html
