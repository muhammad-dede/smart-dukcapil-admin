extends template

block pageVars
    - var pageTitle    = "Layout Boxed"
    - var activeMenu   = "layout-full"

append pageConfig
    - var contentLayout = 'content-right-sidebar'
    - var sidebarSticky = true
    //-OPTIONS:- true, false | For sticky(floating) sidebar, will affect only if there is a sidebar

append breadcrumbs
    +breadcrumbs([{url:"index.html",name:"Home"}, {name:"Layouts"}, {name:"Layout Boxed"}])

//- Include page content in content block
append content
    include ../contents/documentation-layout-full.html

append sidebar
    include ../contents/sidebar/documentation-layout-full-sidebar.html
