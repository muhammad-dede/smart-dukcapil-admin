extends template

block pageVars
    - var pageTitle    = "Starter Kit"
    - var activeMenu   = "starter-kit"



append pageConfig
    - var contentLayout = 'content-right-sidebar'
    - var sidebarSticky = true
    //-OPTIONS:- true, false | For sticky(floating) sidebar, will affect only if there is a sidebar

append breadcrumbs
    +breadcrumbs([{url:"index.html",name:"Home"}, {name:"Starter Kit"}])



//- Include page content in content block
append content
    include ../contents/documentation-starter-kit.html

append sidebar
    include ../contents/sidebar/documentation-starter-kit-sidebar.html
