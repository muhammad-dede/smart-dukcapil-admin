extends template

block pageVars
    - var pageTitle = "Context Menu"
    - var pageSubTitle = "Context Menu"
    - var description = "Context Menu"
    - var activeMenu = "ext-component-context-menu"

block breadcrumbs
    +breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"Extensions"}, {name:"Context Menu"}])

//- Include page content in page block
append content
    include ../contents/ext-component-context-menu.html

//- Vendor CSS
//------------------------------
//- Add vendor specific CSS
append vendorcss
    link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/extensions/jquery.contextMenu.min.css')
    link(rel='stylesheet', type='text/css', href=app_assets_path+'/vendors/css/extensions/toastr.min.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
    script(src=app_assets_path + '/vendors/js/extensions/jquery.contextMenu.min.js')
    script(src=app_assets_path + '/vendors/js/extensions/jquery.ui.position.min.js')
    script(src=app_assets_path+'/vendors/js/extensions/toastr.min.js')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
    script(src=app_assets_path + '/js/scripts/extensions/ext-component-context-menu.js')

//- Page CSS
//------------------------------
//- Add page specific CSS
append pagecss
    link(rel='stylesheet', type='text/css', href=app_assets_path+'/css'+rtl+'/plugins/extensions/ext-component-toastr.css')
    link(rel='stylesheet', type='text/css', href=app_assets_path+'/css'+rtl+'/plugins/extensions/ext-component-context-menu.css')
