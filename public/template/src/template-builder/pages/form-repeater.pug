extends template

block pageVars
  - var pageTitle    = "Form Repeater"
  - var pageSubTitle = "Form Repeater"
  - var description = "Add form using form repeater plugin."
  - var activeMenu   = "form-repeater"

block breadcrumbs
  +breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Forms"}, {name:"Form Repeater"}])

//- Include page content in page block
append content
  include ../contents/form-repeater.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
  script(src=app_assets_path+'/vendors/js/forms/repeater/jquery.repeater.min.js')


//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
  script(src=app_assets_path+'/js/scripts/forms/form-repeater.js')
